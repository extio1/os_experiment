# Num of nodes
N_NODES=5


# Network and mask

NETWORK=192.168.230
MASK=24


# The start address from which hosts are assigned
# e.g. .10, .11, .12, .13, ...

START_ADDR=10


# Network namespace name will be formed as NNS_PREFIX+network_namespace_id,
# where network_namespace_id as index takes values 0, 1, 2, 3, ..., n, ...
# For instance, if NNS_PREFIX=nns_node
# then network namespace names:
# nns_node0, nns_node1, nns_node2, ...

NNS_PREFIX=nnslab


VETH_ROOT_NAME_PREFIX=veth1_mult_lab
# Virtual ethernet interface name in root (and others) network namespace side
VETH_LOCAL_NAME_PREFIX=ceth1_mult_lab


# Bridge interface name
BRIDGE_NAME=br1_mult_lab
